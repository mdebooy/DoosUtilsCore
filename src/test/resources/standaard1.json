{
  "_jar": "ParameterBundle.jar",
  "applicatie": "ParameterBundle",
  "banner": "Parameter Bundle",
  "help": "Hiermee kunnen de parameters voor een applicatie worden gedefinieerd.",
  "parameters": [
    {
      "parameter": "help",
      "kort": "h",
      "lang": "help",
      "help": "Geeft deze helptekst.",
      "verplicht": true
    },
    {
      "parameter": "versie",
      "kort": "v",
      "lang": "versie",
      "help": "Geeft de versie.",
      "standaard": "_@help@_"
    }
  ]
}
